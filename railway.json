{
  "build": {
    "builder": "NIXPACKS"
  },
  "nixpacks": {
    "languages": {
      "nodejs": {
        "version": "18"
      }
    },
    "phases": {
      "install": {
        "cmds": ["npm install"]
      },
      "build": {
        "cmds": ["npm run build"]
      },
      "start": {
        "cmds": ["npm run start"]
      }
    }
  }
}